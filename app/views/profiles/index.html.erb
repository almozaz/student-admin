<div class="row">
  <div class="col-xs-6 col-xs-offset-3">
    <!-- <h1 class="text-center">Take a look at all our users</h1> -->

    <div class="table-responsive">
        <h2 class="text-center title-padding">Admins</h2>
      <table class="table table-hover admin-list" id="admin-list">
        <thead>
          <tr>
            <th>First name</th>
            <th>Last name</th>
            <th class="center">Downgrade</th>
          </tr>
        </thead>
        <tbody>
            <%= render @admins %>
        </tbody>
      </table>
    </div>

    <div class="table-responsive">
        <h2 class="text-center title-padding">Students</h2>
      <table class="table table-hover student-list" id="student-list">
        <thead>
          <tr>
            <th>First name</th>
            <th>Last name</th>
            <th class="center">Upgrade</th>
          </tr>
        </thead>
        <tbody>
            <%= render @students %>
        </tbody>
      </table>
    </div>
  </div>
</div>
